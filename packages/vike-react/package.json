{
  "name": "vike-react",
  "version": "0.4.15",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./useData": "./dist/shared/hooks/useData.js",
    "./usePageContext": "./dist/shared/hooks/usePageContext.js",
    "./ClientOnly": "./dist/shared/components/ClientOnly.js",
    ".": "./dist/shared/index.js",
    "./config": "./dist/shared/+config.js",
    "./renderer/onRenderHtml": "./dist/shared/renderer/onRenderHtml.js",
    "./renderer/onRenderClient": "./dist/shared/renderer/onRenderClient.js",
    "./useNavigate": {
      "browser": "./dist/client/hooks/useNavigate.js",
      "node": "./dist/shared/hooks/useNavigate.js",
      "worker": "./dist/shared/hooks/useNavigate.js",
      "types": "./dist/client/hooks/useNavigate.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist/ && tsc --build",
    "release": "release-me patch",
    "release:minor": "release-me minor",
    "release:commit": "release-me commit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "vike": ">=0.4.173",
    "vite": ">=4.3.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@brillout/release-me": "^0.3.8",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vike": "^0.4.173"
  },
  "dependencies": {
    "react-streaming": "^0.3.27"
  },
  "typesVersions": {
    "*": {
      "useData": [
        "./dist/shared/hooks/useData.d.ts"
      ],
      "usePageContext": [
        "./dist/shared/hooks/usePageContext.d.ts"
      ],
      "useNavigate": [
        "./dist/client/hooks/useNavigate.d.ts"
      ],
      "ClientOnly": [
        "./dist/shared/components/ClientOnly.d.ts"
      ],
      ".": [
        "./dist/shared/index.d.ts"
      ],
      "config": [
        "./dist/shared/+config.d.ts"
      ],
      "renderer/onRenderHtml": [
        "./dist/shared/renderer/onRenderHtml.d.ts"
      ],
      "renderer/onRenderClient": [
        "./dist/shared/renderer/onRenderClient.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "repository": "github:vikejs/vike-react",
  "license": "MIT"
}
